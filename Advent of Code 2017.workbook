```csharp
(int steps, String cells) BuildCells(int wantedCell){
    var wantedCellIndex = wantedCell - 1;
    var cells = new List<(int x, int y)>(wantedCell);
    var x = 0;
    var y = 0;
    var cellIndex = 0;
    var cell = cellIndex + 1;
    cells.Add((x:x,y:y));
    for(var offset = 1; cellIndex < wantedCellIndex; offset++){
        var odd = (offset % 2) == 1;
        if(odd){
            for(var right = 1; right <= offset; right++){
                x+=1;
                cells.Add((x:x,y:y));
            };
            for(var up = 1; up <= offset; up++){
                y+=1;
                cells.Add((x:x,y:y));
            };
        } else { // even
            for(var left = 1; left <= offset; left++){
                x-=1;
                cells.Add((x:x,y:y));
            };
            for(var down = 1; down <= offset; down++){
                y-=1;
                cells.Add((x:x,y:y));
            };            
        }
        cellIndex = cells.Count()-1;
    };
    var pos = cells[wantedCellIndex];
    var steps = Math.Abs(pos.x) + Math.Abs(pos.y);
    var orderedCells = cells.Select((p,i) => (x:p.x,y:p.y,cell:i+1));
    var cellsStr = String.Join(" ",orderedCells.Select(item=>$"{item.cell}:({item.x},{item.y})"));

    return (steps, cellsStr);
}
```

